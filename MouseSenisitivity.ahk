;-------------------------------------------------------------------------; Mouse properties shortcuts;-------------------------------------------------------------------------RAlt & LButton::SetMouseSensitivity(8)RAlt & XButton1::SetMouseSensitivity(4)SetMouseSensitivity(val){  RUn rundll32.exe shell32.dll`,Control_RunDLL main.cpl`,`,2  WinWait, Eigenschaften von Maus  Send {Home}  Loop, %val%  {    SendInput {Right}  }  SendInput {Enter}}